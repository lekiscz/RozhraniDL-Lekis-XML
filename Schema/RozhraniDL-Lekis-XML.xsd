				<![CDATA[
				Součet prodejních cen a rozpis DPH.
				]]>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="prodejniCena"
					type="cenaNullableType"
					minOccurs="1"
					maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="cenaSDPH"
				type="decimalNullableType">
			<xs:annotation>
				<xs:documentation>
					<![CDATA[
					Celkem prodejní cena s DPH.
					]]>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="cenaBezDPH"
				type="decimalNullableType">
			<xs:annotation>
				<xs:documentation>
					<![CDATA[
					Celkem prodejní cena bez DPH.
					]]>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="soucetNakCenType">
		<xs:annotation>
			<xs:documentation>
				<![CDATA[
				Součet nákupních cen a rozpis DPH.
				]]>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nakupniCena"
					type="cenaType"
					minOccurs="1"
					maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="cenaSDPH"
				type="xs:decimal"
				use="required">
			<xs:annotation>
				<xs:documentation>
					<![CDATA[
					Celkem nákupní cena s DPH.
					]]>
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="cenaBezDPH"
				type="xs:decimal"
				use="required">
			<xs:annotation>
				<xs:documentation>
